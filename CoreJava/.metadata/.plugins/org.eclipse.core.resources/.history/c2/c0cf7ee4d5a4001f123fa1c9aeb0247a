package com.in.rohit.Arrays;

public class SimpleArrays {
	
	public static void main(String[] args) {
		 int[] arr = new int [20];
		 
		  arr[3] = 43; 
		  arr[7] = 89 ;
		  arr[8] = 67;
		  
		/*
		 * for( int i = 0 ; i <10 ; i ++) { System.out.println(arr[i]); }
		 */  
		  for(int i =0;   i <arr.length ; i++) {
			  
			  System.out.println(arr [i]);
			  
		  }
	}

}
