package com.in.rohit.Arrays;

public class NumberFound {
	
	 public static void main(String[] args) {
		 int [] arr = {1,2,3,4,55,6,7,8 , -2 };
		 
		  for(int  i : arr) {
			  if(i ==55) {
				  System.out.println("found");
			  }else {
			  
				  System.out.println("not found ");
			  
			  }
				  
		  }
	}

}
